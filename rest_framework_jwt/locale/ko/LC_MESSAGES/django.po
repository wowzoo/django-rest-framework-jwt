#: rest_framework_jwt/authentication.py:35 rest_framework_jwt/serializers.py:88
msgid "Signature has expired."
msgstr "서명이 만료되었습니다."

#: rest_framework_jwt/authentication.py:38 rest_framework_jwt/serializers.py:91
msgid "Error decoding signature."
msgstr "서명을 해독하지 못했습니다."

#: rest_framework_jwt/authentication.py:55
#: rest_framework_jwt/serializers.py:100
msgid "Invalid payload."
msgstr "데이터가 유효하지 않습니다."

#: rest_framework_jwt/authentication.py:61
msgid "Invalid signature."
msgstr "서명이 유효하지 않습니다."

#: rest_framework_jwt/authentication.py:65 rest_framework_jwt/serializers.py:54
#: rest_framework_jwt/serializers.py:111
msgid "User account is disabled."
msgstr "사용자 계정을 사용할 수 없습니다."

#: rest_framework_jwt/authentication.py:89
msgid "Invalid Authorization header. No credentials provided."
msgstr "잘못된 인증 헤더. 자격 증명이 제공되지 않았습니다."

#: rest_framework_jwt/authentication.py:92
msgid ""
"Invalid Authorization header. Credentials string should not contain spaces."
msgstr "잘못된 인증 헤더. 자격 증명 문자열은 공백을 포함하지 않아야합니다."

#: rest_framework_jwt/serializers.py:64
msgid "Unable to login with provided credentials."
msgstr "제공한 자격 증명을 사용하여 로그인 할 수 없습니다."

#: rest_framework_jwt/serializers.py:67
#, python-brace-format
msgid "Must include \"{username_field}\" and \"password\"."
msgstr "\"{username_field}\" 과(와) \"password\" 를(을) 포함해야 합니다."

#: rest_framework_jwt/serializers.py:107
msgid "User doesn't exist."
msgstr "사용자가 존재하지 않습니다."

#: rest_framework_jwt/serializers.py:159
msgid "Refresh has expired."
msgstr "새로 고침이 만료되었습니다."

#: rest_framework_jwt/serializers.py:162
msgid "orig_iat field is required."
msgstr "orig_iat 필드가 필요합니다."
